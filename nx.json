{
  "defaultBase": "master",
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "build:client": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "build:write-version-file": {
      "cache": true,
      "dependsOn": ["build"]
    },
    "lint": {
      "cache": true
    },
    "test": {
      "dependsOn": ["^build", "test:client-generation-clean"],
      "cache": true
    }
  }
}
