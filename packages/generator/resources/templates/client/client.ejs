import * as operations from "./operations.js";
import ApiClientBase from "@mittwald/api-client-commons/dist/core/ApiClientBase.js";

export class <%- rootNamespace %>Client extends ApiClientBase {
    <% Object.entries(paths).forEach(([path, operations]) => { %>
        <% Object.entries(operations).forEach(([method, operation]) => { %>
            public <%- operationId(path, method, operation) %> = this.requestFunctionFactory(operations.<%- operationId(path, method, operation) %>);
        <% }) %>
    <% }) %>
}

export default <%- rootNamespace %>Client;
