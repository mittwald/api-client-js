import { <%- rootNamespace %> } from "./types";
import { Simplify } from "@mittwald/api-client-commons/dist/type-fest";
import { Request } from "@mittwald/api-client-commons/dist/types/Request";
import { Response } from "@mittwald/api-client-commons/dist/types/Response";
import { OpenAPIOperation } from "@mittwald/api-client-commons/dist/types/OpenAPIOperation";

<% Object.entries(paths).forEach(function([pathname, path]) { %>
    <% Object.entries(path).forEach(function([method, operation]) { %>
        export const <%- operationId(pathname, method, operation) %>: OpenAPIOperation<
        <%- include('../descriptor/request', {method, pathname, operation}); %>,
        <%- include('../descriptor/response', {method, pathname, operation}); %>
        > = {
            path: "<%- pathname %>",
            method: "<%- method.toUpperCase() %>",
            operationId: "<%- operationId(pathname, method, operation) %>",
        };
    <% }) %>
<% }) %>

