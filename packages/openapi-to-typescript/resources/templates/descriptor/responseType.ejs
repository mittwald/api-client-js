<%- !operation.responses || Object.keys(operation.responses).length === 0 ? "never": "" %>
<% Object.entries(operation.responses).forEach(function([status, response]) { %>
    <% if (response.content && Object.keys(response.content).length > 0) { %>
        <% Object.entries(response.content).forEach(function([mediaType, content]) { %>
            | ({status: <%- status === "default" ? 0 : status %>} & {
                mediaType: "<%- mediaType %>";
                content: <%- joinNamespace(rootNamespace, "paths", pathname, method, "responses", status, "content", mediaType) %>;
                <% if (response.headers) { %>
                    headers: <%- joinNamespace(rootNamespace, "paths", pathname, method, "responses", status, "headers") %>;
                <% }%>
            })
        <% }) %>
    <% } else { %>
    | ({status: <%- status === "default" ? 0 : status %>} & {
        <% if (response.headers) { %>
            headers: <%- joinNamespace(rootNamespace, "paths", pathname, method, "responses", status, "headers") %>;
        <% }%>
        content: never;
    })
    <% } %>
<% }) %>
