{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@mittwald/api-client-commons/*": ["packages/commons/*"]
    },
    "stripInternal": true,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "composite": true
  }
}
